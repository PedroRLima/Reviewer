<div class="container">
<h1>Professores</h1>

<% @professors.each do |prof| %>
<%= image_tag(prof.avatar.url(:medium), :class => "img-rounded") %>
<%=prof.name%>
<%= link_to "Avaliar", prof %>
<% end %>

<% if user_signed_in? %>
	<%= current_user.email %>
	<%= link_to "logout" , destroy_user_session_path, :method => :delete %>
<% end %>
</div>